<div class="container form">
  <h1><i class="fas fa-cocktail"></i> | Create new Cocktail </h1>
  <%= simple_form_for(@cocktail) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Add Cocktail", :class => "btn btn-grey"  %>
  <% end %>
</div>
